<h1>Listing tipo_vagas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_vagas.each do |tipo_vaga| %>
      <tr>
        <td><%= tipo_vaga.nome %></td>
        <td><%= link_to 'Show', tipo_vaga %></td>
        <td><%= link_to 'Edit', edit_tipo_vaga_path(tipo_vaga) %></td>
        <td><%= link_to 'Destroy', tipo_vaga, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo vaga', new_tipo_vaga_path %>
