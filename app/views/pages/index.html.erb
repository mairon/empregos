<section class="segundo_top">	<section class="cadastro">	<h2>CADASTRO RÁPIDO</h2>	<div class="tabbable tabbable-bordered">		<ul class="nav nav-tabs">			<li class="active"><a href="#tab_br1" data-toggle="tab">Candidato</a></li>			<li><a href="#tab_br1" data-toggle="tab">Empresa</a></li>		</ul>		<div class="tab-content">			<div class="tab-pane active" id="tab_br1">				<%= render  "/devise/registrations/new"%>			</div>			<div class="tab-pane" id="tab_br2">			</div>		</div>	</div>	</section>	<section class="banner">	  <div class="slide">	    <div class="cycle-slideshow" 	        data-cycle-fx=scrollHorz	        data-cycle-timeout=5000	        data-cycle-pager="#custom-pager"	        data-cycle-pager-template="<a href=#>{{slideNum}}</a>"	        >	        <%= image_tag("Empregos.png") %>	        <%= image_tag("Empregos.png") %>	    </div>	  </div>	</section>	<section class="find_page">		<div class="input_search">			<div class="title_search"><%= image_tag("gCons/search.png") %> Buscar</div>			<input type="search" name="" value="" placeholder="Qual seu próximo emprego?">			</div>		<div class="button_search">			<input type="submit" name="" value="Encontrar">		</div>	</section>	<menu>		<li><a href="" title="">QUEM SOMOS</a></li>		<li><a href="" title="">CURRICULOS</a></li>		<li><a href="" title="">VAGAS</a></li>		<li><a href="" title="">CADASTRE-SE</a></li>		<li><a href="" title="">ONDE ESTAMOS</a></li>		<li><a href="" title="">SUPORTE</a></li>		<li><a href="" title="">BLOG</a></li>		<li><a href="" title="">FAQ</a></li>		<li><a href="" title="">CONTATO</a></li>	</menu></section><section class="lists">	<article class="list_ultimas_vagas">	<h2>ÚLTIMAS VAGAS CADASTRADAS</h2>		<nav>			<ul>			<% @ultimas_vagas.each do |uv| %>				<a href="" title="">					<li>						<%= uv.cargo.nome %><br>						<%= uv.empresa.city.name %>					</li>				</a>							<% end %>			</ul>		</nav>		</article>	<article class="list_empresas">		<h2>EMPRESAS CADASTRADAS</h2>	</article>	<article class="list_ultimos_curriculoas">		<h2>CURRÍCULOS CADASTRADOS</h2>		<nav>			<ul>			<% @curriculos_vagas.each do |cv| %>				<a href="" title="">				<li>					<%= cv.cargo.nome %><br>					<%= cv.curriculos %> curriculos				</li>				</a>			<% end %>			</ul>		</nav>	</article></section><section class="info">	<%= image_tag("infografico.png") %></section>